#!/usr/bin/env node

var fs = require("fs")
var path = require("path")
var spawnSync = require("child_process").spawnSync

var main = path.dirname(path.dirname(fs.realpathSync(process.argv[1]))) + "/src/main.js"
var child = spawnSync("node", [main].concat(process.argv.slice(2)), { stdio: "inherit" })
process.exit(child.status)
